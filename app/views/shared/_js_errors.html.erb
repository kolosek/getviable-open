<% if target.errors.any? %>
  <div id="ajax_error_explanation">
    <!--<h2><%#= pluralize(model.errors.count, "error") %> occured:</h2> -->
    <ul class = "ajax_errors">
    <% target.errors.full_messages.each do |msg| %>
      <li class= "list" style= "color:red;"><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>